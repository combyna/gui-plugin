{
    "name": "combyna/gui-plugin",
    "description": "A plugin for Combyna that provides basic GUI components",
    "autoload": {
        "psr-4": {
            "Combyna\\Plugin\\Gui\\": "php/src",
            "Combyna\\": "php/dist/Combyna"
        }
    },
    "config": {
        "platform": {
            "php": "7.1"
        }
    },
    "scripts": {
        "build:expression-parser": "mkdir -p ./php/dist/Combyna/Component/ExpressionLanguage && peg ./vendor/combyna/combyna/php/src/Combyna/Component/ExpressionLanguage/PegExpressionParser.php.inc ./php/dist/Combyna/Component/ExpressionLanguage/PegExpressionParser.php"
    },
    "require-dev": {
        "combyna/combyna": "dev-rush",
        "phpunit/phpunit": "^4.6"
    }
}
